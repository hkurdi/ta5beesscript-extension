{
  "name": "ta5bees",
  "displayName": "Ta5beesScript",
  "description": "Ta5beesScript language support for Visual Studio Code",
  "version": "0.0.1",
  "publisher": "whoishlk", 
  "license": "MIT",
  "readme": "README.md",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ta5bees-extension.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ta5bees-extension/issues"
  },
  "homepage": "https://github.com/yourusername/ta5bees-extension#readme",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:ta5beesscript"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ta5beesscript",
        "aliases": [
          "Ta5beesScript",
          "ta5beesscript"
        ],
        "extensions": [
          ".ta5bees"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ta5beesscript",
        "scopeName": "source.ta5bees",
        "path": "./syntaxes/ta5beesscript.tmLanguage.json"
      }
    ],
    "capabilities": {
      "completionProvider": {
        "resolveProvider": true,
        "triggerCharacters": [
          ".",
          "("
        ]
      }
    },
    "iconThemes": [
      {
        "id": "ta5bees-icons",
        "label": "Ta5bees Icons",
        "path": "./icons/ta5bees-icon-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "package": "vsce package",
    "publish": "vsce publish",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "eslint": "^8.50.0",
    "vsce": "^2.16.0"
  }
}
