{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Ta5beesScript",
    "scopeName": "source.ta5bees",
    "patterns": [
        { "include": "#keywords" },
        { "include": "#strings" },
        { "include": "#comments" },
        { "include": "#functions" },
        { "include": "#variables" }
    ],
    "repository": {
        "keywords": {
            "patterns": [
                {
                    "name": "keyword.control.js",
                    "match": "\\b(khalli|thabit|arrifli|iza|willa|la|raje3li|hawel|law_sar_ma_sar|irmi|kammil|iksir|stanna|tazamonan)\\b"
                }
            ]
        },
        "strings": {
            "name": "string.quoted.double.js",
            "begin": "\"",
            "end": "\"",
            "patterns": [
                {
                    "name": "constant.character.escape.js",
                    "match": "\\\\."
                }
            ]
        },
        "comments": {
            "patterns": [
                {
                    "name": "comment.line.double-slash.js",
                    "match": "//.*$"
                }
            ]
        },
        "functions": {
            "patterns": [
                {
                    "name": "entity.name.function.js",
                    "match": "\\b(arrifli|tazamonan\\s+arrifli)\\s+([_$a-zA-Z][_$a-zA-Z0-9]*)\\s*\\("
                }
            ]
        },
        "variables": {
            "patterns": [
                {

                    "name": "storage.type.js",
                    "match": "\\b(khalli|thabit)\\b"
                },
                {

                    "name": "variable.other.readwrite.js",
                    "match": "\\b([_$a-zA-Z][_$a-zA-Z0-9]*)\\b"
                }
            ]
        }
    }
}
